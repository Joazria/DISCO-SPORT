    <%= favicon_link_tag 'favicon.ico' %>
    <meta property="og:image" content="https://res.cloudinary.com/numama/image/upload/v1617660870/DISCOSPORT/linkedin_post_1_discop_ozm3jp.png">
<meta property="og:url" content="www.discosport.tv">
